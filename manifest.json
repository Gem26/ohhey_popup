{
    "name": "OhHey",
    "description": "Social Media Extension",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage"],
    "action": {
      "default_popup": "ohhey.html",
      "default_icon": "Icon.png"
    },
    "content_scripts": [
      {
        "matches": ["https://www.facebook.com/"],
        "js": ["content-script.js"]
      }
    ]
  }